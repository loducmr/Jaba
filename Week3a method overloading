public class week3a {
void add(int a,int b )
{
    System.out.println(a+b);
}
void add(int a,int b,int c)
{
    System.out.println(a+b+c);
}
void add(String a,String b)       
{
        System.out.println(a+b);
}
public static void main(String args[])
{
    week3a x=new week3a();
    x.add("hai","hello");
    x.add(20,30);
    x.add(10,20,30);
    }
}

import java.util.Scanner;
public class week1b {
     public static void main(String[] args){
     int n,c = 0,i,j;
     Scanner s=new Scanner(System.in);
     System.out.println("enter n value");
     n=s.nextInt();
     System.out.println("prime numbers upto n:");
     for ( i=1;i<=n;i++)
     {
         c =0;
     for( j=1;j<=i;j++)
     {
         if(i%j==0)
         {
             c++;
         }
     }
     if(c==2)
     {
         System.out.println(i);
     }
     }
}
}
